<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lishuai.mapper.ComplaintMapper">

    <update id="answerComplaint">
        update complaint set flag =1,answer=#{answer} where complaint_id = #{complaintId}
    </update>

</mapper>
